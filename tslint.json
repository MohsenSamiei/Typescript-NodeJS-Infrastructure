{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**",
            "build/**"
        ]
    },
    "jsRules": {},
    "rules": {
        "trailing-comma": false,
        "class-name": false,
        "variable-name": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "no-console": false,
        "eofline": false
    },
    "rulesDirectory": []
}